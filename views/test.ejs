<% include ./partials/head-tags %>
<% include ./partials/main-menu %>
    
  
 <div class="container">   
<div class="row mt-3 mb-3">

    <div class="col-lg-6 mb-3 ">
      <div class="col-lg-12 bestBuyBoxes">
        <h4 class="mt-3">
          Latest Residential Best Buys
        </h4>
        <p>
         <strong>Based on a mortgage of £125,000 at 50% LTV</strong> 
        </p>
        
        <table class="mb-3">
          <thead>
            <tr>
             <th style="width: 40%">Provider</th>
             <th style="width: 20%">Initial Rate</th>
             <th style="width: 40%">Max LTV (Loan to Value)</th>
            </tr>
          </thead>
            <% var i %>
            <% for(i = 0; i <= 2; i++ ){ %>
            <tr>
             <td> <%= rate[i].lender %> </td>
             <td> <%= rate[i].rate %> </td>
             <td> <%= rate[i].ltv %> </td>
            </tr>
            <% } %>     
        </table>
      </div>
    </div>
    <div class="col-lg-6 ">
      <div class="col-lg-12 bestBuyBoxes">
        <h4 class="mt-3">
          Latest Buy to Let Best Buys
        </h4>
        <P>
          <strong>Based on a mortgage of £125,000 at 50% LTV</strong>
        </P>
        
        <table class="mb-3">
          <thead>
            <tr>
             <th style="width: 40%">Provider</th>
             <th style="width: 20%">Initial Rate</th>
             <th style="width: 40%">Max LTV (Loan to Value)</th>
            </tr>
          </thead>
            <% var i %>
            <% for(i = 0; i <= 2; i++ ){ %>
            <tr>
             <td> <%= rate[i].lender %> </td>
             <td> <%= rate[i].rate %> </td>
             <td> <%= rate[i].ltv %> </td>
            </tr>
            <% } %>     
        </table>        
      </div>
    </div>
</div>
</div>
    
    
  
    
    
    
