<% include ../partials/head-tags %>
<% include ../partials/main-menu %>


<div class="container"> 
 <div class="rows">
   <div class="col">  
         <h3>
           Edit <%= foundbtlPost.title %>  Post
         </h3>

     <form action="/buy-to-let-mortgages/<%= foundbtlPost._id%>/?_method=PUT" method="POST">

           <div class="form-group">
       
           <div class="form-group">
             <label for="Customizable URL">Customizable URL</label>
              <input type="text" class="form-control " value="<%=foundbtlPost._id%>" name="buytolet[_id]">
           </div>
   
           <div class="form-group">
             <label for="Image URL">Image URL</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundbtlPost.image%>" name="buytolet[image]">
           </div>
           
           <div class="form-group">
             <label for="Post Title">Post Title</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundbtlPost.title%>" name="buytolet[title]">
           </div>
       
           <div class="form-group">
             <label for="Post MetaTitle">Post MetaTitle</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundbtlPost.metatitle%>" name="buytolet[metatitle]">
           </div>
       
             <div class="form-group">
               <label for="Meta Description">Post Meta Description</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundbtlPost.metadescription%>" name="buytolet[metadescription]">
           </div>
           
           <div class="form-group">
             <label for="Content">Post Content</label>
             <textarea rows="10" class="form-control form-control-lg" name="buytolet[content]"><%=foundbtlPost.content%></textarea>
           </div>
             
       

           <button class="btn btn-outline-primary" type="submit">Submit</button>
  
         </form>
     
   </div>
  </div>
</div>



<% include ../partials/footer %>