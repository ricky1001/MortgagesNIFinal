<% include ../partials/head-tags %>
<% include ../partials/main-menu %>


<div class="container"> 
 <div class="rows">
   <div class="col">  
         <h3>
           Edit <%= foundmtPost.title %>  Post
         </h3>

     <form action="/mortgage-types/<%= foundmtPost._id%>/?_method=PUT" method="POST">

           <div class="form-group">
       
           <div class="form-group">
             <label for="Customizable URL">Customizable URL</label>
              <input type="text" class="form-control " value="<%=foundmtPost._id%>" name="mortgageType[_id]">
           </div>
   
           <div class="form-group">
             <label for="Image URL">Image URL</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundmtPost.image%>" name="mortgageType[image]">
           </div>
           
           <div class="form-group">
             <label for="Post Title">Post Title</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundmtPost.title%>" name="mortgageType[title]">
           </div>
       
           <div class="form-group">
             <label for="Post MetaTitle">Post MetaTitle</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundmtPost.metatitle%>" name="mortgageType[metatitle]">
           </div>
       
             <div class="form-group">
               <label for="Meta Description">Post Meta Description</label>
             <input type="text" class="form-control form-control-lg" value="<%=foundmtPost.metadescription%>" name="mortgageType[metadescription]">
           </div>
           
           <div class="form-group">
             <label for="Content">Post Content</label>
             <textarea rows="10" class="form-control form-control-lg" name="mortgageType[content]"><%=foundmtPost.content%></textarea>
           </div>
             
       

           <button class="btn btn-outline-primary" type="submit">Submit</button>
  
         </form>
     
   </div>
  </div>
</div>



<% include ../partials/footer %>